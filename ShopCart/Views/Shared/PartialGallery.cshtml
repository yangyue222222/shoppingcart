@using ShopCart.Models

<table>
    @foreach (var product in (List<Product>)ViewData["partial"])
    {
        <tr>
            <td>@product.pro_id</td>
            <td>@product.pro_name</td>
            <td>@product.pro_desc</td>
            <td>@product.pro_price</td>
            <td><img src="~/images/@product.pro_image" /></td>
        </tr>
    }


</table>
